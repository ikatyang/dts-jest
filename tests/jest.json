{
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "js", "json"],
  "rootDir": "..",
  "testMatch": ["<rootDir>/tests/**/*.ts"],
  "transform": {
    "\\.tsfm\\.ts$": "<rootDir>/transform",
    "\\.tsfm-act\\.ts$": "<rootDir>/transform-actual"
  },
  "reporters": [
    "default",
    "<rootDir>/reporter"
  ]
}
