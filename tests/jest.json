{
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "js", "json"],
  "rootDir": "..",
  "testMatch": ["<rootDir>/tests/example.ts"],
  "transform": {
    "\\.ts$": "<rootDir>/transform"
  },
  "reporters": ["default", "<rootDir>/reporter"],
  "globals": {
    "_dts_jest_": {
      "compiler_options": {
        "module": "commonjs",
        "strict": true,
        "target": "es6"
      }
    }
  }
}
