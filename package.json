{
  "name": "dts-jest",
  "version": "21.0.0",
  "description": "A preprocessor for Jest to snapshot test TypeScript declaration (.d.ts) files",
  "keywords": [
    "jest",
    "jest-transform",
    "test",
    "typescript",
    "typescript-declarations"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "dts-jest-remap": "bin/dts-jest-remap.js"
  },
  "repository": "https://github.com/ikatyang/dts-jest",
  "homepage": "https://github.com/ikatyang/dts-jest#readme",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run build",
    "lint": "tslint --type-check -p ./tsconfig.json",
    "prebuild": "rm -rf ./lib",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "jest -c ./jest.json --no-cache",
    "test-integration": "jest -c ./tests/jest.json --no-cache",
    "snap-integration": "yarn run test-integration -- --verbose --useStderr --no-color --noStackTrace 2>&1 >/dev/null | (ts-node ./scripts/normalize-jest-output.ts) > ./snapshots/integration.txt",
    "release": "standard-version",
    "check-diff": "git diff --exit-code"
  },
  "dependencies": {
    "globby": "^6.1.0",
    "jest-snapshot-parser": "^1.0.0",
    "lodash.intersection": "^4.4.0",
    "make-dir": "^1.0.0",
    "pretty-format": "^20.0.3",
    "ts-comment": "^1.1.0",
    "tsconfig-extends": "^1.0.1",
    "tslib": "^1.7.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/globby": "0.6.0",
    "@types/jest": "20.0.8",
    "@types/lodash.intersection": "4.4.3",
    "@types/make-dir": "1.0.1",
    "@types/node": "8.0.25",
    "@types/pretty-format": "20.0.0",
    "@types/yargs": "8.0.2",
    "jest": "20.0.4",
    "prettier": "1.5.3",
    "prettier-config-ikatyang": "1.1.1",
    "standard-version": "4.2.0",
    "ts-jest": "20.0.10",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-config-ikatyang": "2.2.0",
    "tslint-config-prettier-ext": "1.5.0",
    "tslint-plugin-prettier": "1.1.0",
    "typescript": "2.4.2"
  },
  "peerDependencies": {
    "jest": "^20.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "/bin/**/*",
    "/lib/**/*",
    "/*.js"
  ]
}
