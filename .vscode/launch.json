{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/node_modules/jest-cli/bin/jest.js",
      "args": [
        "--runInBand",
        "--config",
        "./jest.json",
        "--no-cache"
      ],
      "protocol": "inspector",
      "outFiles": ["${workspaceRoot}/lib/**/*.js"]
    }
  ]
}
